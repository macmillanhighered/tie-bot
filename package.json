{
  "name": "tie-bot",
  "version": "1.0.3",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "CONSUL_PATH=dev/ NODE_ENV=development babel-watch src/index.js & yarn client-dev",
    "build": "rm -rf ./dist/ && babel src --out-dir dist/ --copy-files && yarn client-build && copyfiles package.json dist/",
    "prod": "NODE_ENV=production node dist/index.js",
    "deploy": "git push dokku master",
    "logs": "dt logs -t",
    "lint": "eslint src --fix",
    "test": "mocha -w --require babel-core/register --watch-extension .js",
    "client-dev": "webpack-dev-server --mode development --history-api-fallback --content-base src/client/static/",
    "client-build": "BABEL_DISABLE_CACHE=1 webpack --config webpack.config.babel.js --mode development",
    "heroku-postbuild": "yarn build",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-watch": "^2.0.7",
    "body-parser": "1.19.0",
    "chance": "1.1.6",
    "consul": "^0.33.1",
    "copy-webpack-plugin": "5.1.2",
    "copyfiles": "2.3.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "express": "4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-unfetch": "^2.0.0",
    "js-base64": "2.5.2",
    "moment": "2.26.0",
    "morgan": "^1.9.0",
    "octonode": "^0.9.2",
    "polished": "^1.9.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "request": "2.88.2",
    "styled-components": "^3.3.2",
    "tinytime": "^0.2.6",
    "tracer": "^1.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.6.0",
    "yup": "0.28.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true,
      "cy": true,
      "describe": true
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "global-require": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "react/prop-types": "warn",
      "no-continue": "off",
      "no-underscore-dangle": "off",
      "import/no-dynamic-require": "off",
      "import/prefer-default-export": "off",
      "generator-star-spacing": "off",
      "import/no-extraneous-dependencies": "off",
      "react/no-unused-prop-types": "off",
      "max-len": [
        "warn"
      ],
      "no-param-reassign": [
        "warn",
        {
          "props": false
        }
      ],
      "no-restricted-globals": "off",
      "camelcase": "off",
      "jsx-a11y/label-has-for": "off",
      "react/jsx-no-target-blank": "off",
      "no-throw-literal": "off",
      "no-plusplus": "off",
      "import/extensions": "off",
      "react/jsx-filename-extension": "off",
      "react/forbid-prop-types": "off",
      "react/no-danger": "off",
      "no-useless-escape": "off",
      "jsx-a11y/no-static-element-interactions": "warn",
      "no-unused-expressions": [
        "error",
        {
          "allowTaggedTemplates": true
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1"
  }
}
